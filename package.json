    }
  ],
  "main": "lib/module.js",
  "repository": "https://github.com/",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --fix lib test",
    "start": "npm run build && npm run start:server",
    "start:server": "nuxt start -c test/fixture/nuxt.config.js",
    "dev": "nuxt -c test/fixture/nuxt.config.js",
    "build": "nuxt build -c test/fixture/nuxt.config.js",
    "analyze": "nuxt build --analyze -c test/fixture/nuxt.config.js",
    "test": "npm run lint && jest",
    "codecov": "codecov",
    "release": "yarn test && standard-version && git push --follow-tags && yarn publish"
  },
  "eslintIgnore": [
    "lib/templates/*.*"
  ],
  "files": [
    "lib"
  ],
  "dependencies": {
    "wpapi": "^1.2.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "1.1.2",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "codecov": "^3.5.0",
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "jest": "^24.9.0",
    "nuxt": "^2.9.2",
    "standard-version": "^7.0.0"
  }
}
